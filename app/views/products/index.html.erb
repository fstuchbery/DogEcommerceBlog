<h1>Finns Dog Store</h1>
<div class = "linksIndex">
<%= link_to 'Cart', cart_path %>

<%= render 'shared/category_link' %>

 <%= render 'shared/search_link' %>

 <%= render 'shared/admin_link' %>
</div>

<div id = "productsList">
<ul>
  <% @products.each do |product| %>
    <li>
      <%= link_to product.product_name, product_path(product) %>
    </li>
  <% end %>
</ul>
</div>